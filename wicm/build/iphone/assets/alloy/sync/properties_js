function S4(){return(0|65536*(1+Math.random())).toString(16).substring(1)}function guid(){return S4()+S4()+"-"+S4()+"-"+S4()+"-"+S4()+"-"+S4()+S4()+S4()}function Sync(e,t,o){var i=t.config.adapter.collection_name?t.config.adapter.collection_name:"default",l=new RegExp("^("+i+")\\-(.+)$"),a=null;if("read"===e)if(o.parse){var r=[];_.each(TAP.listProperties(),function(e){var t=e.match(l);null!==t&&r.push(TAP.getObject(e))}),a=r}else{var n=TAP.getObject(i+"-"+t.id);t.set(n),a=t.toJSON()}else"create"===e||"update"===e?(t.id||(t.id=guid(),t.set(t.idAttribute,t.id)),TAP.setObject(i+"-"+t.id,t.toJSON()||{}),a=t.toJSON()):"delete"===e&&(TAP.removeProperty(i+"-"+t.id),t.clear(),a=t.toJSON());a?(_.isFunction(o.success)&&o.success(a),"read"===e&&t.trigger("fetch")):_.isFunction(o.error)&&o.error(a)}var Alloy=require("alloy"),_=require("alloy/underscore")._,TAP=Ti.App.Properties;module.exports.sync=Sync,module.exports.beforeModelCreate=function(e){return e=e||{},e.columns=e.columns||{},e.defaults=e.defaults||{},("undefined"==typeof e.columns.id||null===e.columns.id)&&(e.columns.id="String"),e};