function Controller(){function e(){if(Alloy.Globals.APIKey){Alloy.Globals.loading.show("Loading, Please wait..",!0);var t=Ti.Network.createHTTPClient();t.open("GET",Alloy.Globals.serverPath+"/api/things/mine"),t.setRequestHeader("Authorization","Token token="+Alloy.Globals.APIKey),t.send(),t.onload=function(){var e=JSON.parse(this.responseText);_.each(e.result,function(e,t){var o=parseFloat(e.cool_score).toFixed(2),i={score:o,image:e.url};0==t%2?a.col1.add(Alloy.createController("thing",i).getView()):a.col2.add(Alloy.createController("thing",i).getView())}),Alloy.Globals.loading.hide()},t.onerror=function(){Ti.API.info("got some error"),Alloy.Globals.loading.hide()}}else Alloy.Globals.loginFirst(e)}function t(e){e.cancelBubble=!0}function o(e){e.cancelBubble=!0}function i(e){e.cancelBubble=!0}function l(e){e.cancelBubble=!0}require("alloy/controllers/BaseController").apply(this,Array.prototype.slice.call(arguments)),this.__controllerPath="things",arguments[0]?arguments[0].__parentSymbol:null,arguments[0]?arguments[0].$model:null,arguments[0]?arguments[0].__itemTemplate:null;var a=this,r={},n={};a.__views.coolest=Ti.UI.createView({id:"coolest",layout:"vertical",bottom:"45"}),a.__views.coolest&&a.addTopLevelView(a.__views.coolest),t?a.__views.coolest.addEventListener("touchmove",t):n["$.__views.coolest!touchmove!onTouchmove"]=!0,o?a.__views.coolest.addEventListener("touchstart",o):n["$.__views.coolest!touchstart!onTouchstart"]=!0,i?a.__views.coolest.addEventListener("touchend",i):n["$.__views.coolest!touchend!onTouchend"]=!0,l?a.__views.coolest.addEventListener("swipe",l):n["$.__views.coolest!swipe!onSwipe"]=!0,a.__views.scrollView=Ti.UI.createScrollView({id:"scrollView",layout:"horizontal"}),a.__views.coolest.add(a.__views.scrollView),a.__views.col1=Ti.UI.createView({id:"col1",width:"45%",layout:"vertical"}),a.__views.scrollView.add(a.__views.col1),a.__views.col2=Ti.UI.createView({id:"col2",width:"55%",layout:"vertical"}),a.__views.scrollView.add(a.__views.col2),r.de